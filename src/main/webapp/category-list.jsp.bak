<%@page contentType="text/html;charset=utf-8" %>
<%@page import="bean.*" %>
<jsp:useBean id ="sdto" scope="request" class="bean.ConnectionManager" />

<html>
  <head>
    <title>カテゴリ一覧</title>
  </head>
<body>

<h2>商品情報の表示</h2>
<table border="0">
  <tr>
    <th width="50">番号</th>
    <th width="50">商品名</th>
  </tr>

<%
  for(int i = 0; i < sdto.size(); i++){
	  CategoryBean sb = sdto.get(i);
%>
  <tr>
    <td align="center"><%= sb.getId() %></td>
    <td align="center"><%= sb.getName() %></td>
  </tr>
<% } %>
</table>
</body>
</html>